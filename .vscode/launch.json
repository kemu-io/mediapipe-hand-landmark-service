  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Processor",
      "runtimeVersion": "22.2.0",
      "preLaunchTask": "pre-launch-task",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/dist/processor.js",
      "outFiles": ["${workspaceFolder}/**/*.js"],
      // "console": "integratedTerminal",
      "envFile": "${workspaceFolder}/.env",
      "env": {
        "DEBUG": "kemuWidgetService",
        // "NO_INVOKE_TIMEOUT": "true",
      }
    },
  ]
}